{
    "go_keyvalue_log" : {
        "title" : "key value log format",
        "description" : "key=value",
        "url" : "http://example.com/doesnextis.html",
        "regex" : {
            "basic" : {
                "pattern" : "^(?<timestamp>\\d{4}/\\d{2}/\\d{2} \\d{2}:\\d{2}:\\d{2}) (?<file>\\w+.\\w+:\\d+:) (?<level>\\w+:) (?<body>(\\w+=.+)+)$"
            }
        },
        "level-field" : "level",
        "level" : {
            "error" : "ERROR",
            "warning" : "WARNING",
            "debug" : "DEBUG",
            "info": "INFO"
        },
        "value" : {
            "component" : {
                "kind" : "string",
                "identifier" : true
            }
        },
        "sample" : [
            {
                "line" : "2011/04/01 15:14:34 toto.go:69: ERROR: msg=Shit's on fire yo!"
            }
        ]
    }
}
